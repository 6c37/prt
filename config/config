# Set location of portdir
set portdir /usr/ports

# Repo order, the port found first is used
set order core 6c37-git 6c37 opt xorg contrib

# Alias ports
alias opt/freetype 6c37/freetype-iu
alias opt/fontconfig 6c37/fontconfig-iu
alias opt/cairo 6c37/cairo-iu
alias opt/gtk 6c37/gtk-ahoka
alias opt/glib 6c37/glib-ahoka
alias core/openssl 6c37/libressl

# pkgmk command
set makecommand pkgmk
